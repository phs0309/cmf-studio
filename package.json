{
  "name": "ai-cmf-designer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build && npm run server:build",
    "preview": "vite preview",
    "start": "cd server && npm start",
    "dev:full": "concurrently \"npm run server:dev\" \"npm run dev\"",
    "server:dev": "cd server && npm run dev",
    "server:build": "cd server && npm install && npm run build",
    "server:start": "cd server && npm start"
  },
  "dependencies": {
    "react-dom": "^19.1.1",
    "@google/genai": "^1.17.0",
    "react": "^19.1.1",
    "vite": "^6.2.0",
    "typescript": "~5.8.2"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "concurrently": "^8.2.2"
  }
}